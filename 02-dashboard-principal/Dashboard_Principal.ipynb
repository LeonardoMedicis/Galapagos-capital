    "            \\"categoria\\": \\"rentabilidade\\",\\n",
    "            \\"ultima_execucao\\": datetime.now().isoformat(),\\n",
    "            \\"sucesso\\": True,\\n",
    "            \\"tempo_execucao\\": 2.5,\\n",
    "            \\"usuario\\": \\"sistema\\",\\n",
    "            \\"erro\\": None\\n",
    "        },\\n",
    "        {\\n",
    "            \\"nome\\": \\"IR_Retido_Fonte\\",\\n",
    "            \\"categoria\\": \\"impostos\\",\\n",
    "            \\"ultima_execucao\\": datetime.now().isoformat(),\\n",
    "            \\"sucesso\\": False,\\n",
    "            \\"tempo_execucao\\": 1.2,\\n",
    "            \\"usuario\\": \\"sistema\\",\\n",
    "            \\"erro\\": \\"Arquivo n√£o encontrado\\"\\n",
    "        },\\n",
    "        {\\n",
    "            \\"nome\\": \\"Custodia_Titulos\\",\\n",
    "            \\"categoria\\": \\"outras\\",\\n",
    "            \\"ultima_execucao\\": None,\\n",
    "            \\"sucesso\\": None,\\n",
    "            \\"tempo_execucao\\": None,\\n",
    "            \\"usuario\\": None,\\n",
    "            \\"erro\\": None\\n",
    "        }\\n",
    "    ]\\n",
    "}\\n",
    "\\n",
    "@app.route('/')\\n",
    "def dashboard():\\n",
    "    \\"\\"\\"P√°gina principal do dashboard.\\"\\"\\"\\n",
    "    return render_template('dashboard_adaptavel.html', environment=ambiente)\\n",
    "\\n",
    "@app.route('/api/status')\\n",
    "def api_status():\\n",
