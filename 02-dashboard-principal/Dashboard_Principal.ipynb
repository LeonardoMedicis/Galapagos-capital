   "source": [
    "import sys\\n",
    "import subprocess\\n",
    "import importlib\\n",
    "\\n",
    "def instalar_se_necessario(pacote):\\n",
    "    try:\\n",
    "        importlib.import_module(pacote)\\n",
    "        print(f\\"‚úÖ {pacote} j√° instalado\\")\\n",
    "        return True\\n",
    "    except ImportError:\\n",
    "        print(f\\"üì¶ Instalando {pacote}...\\")\\n",
    "        subprocess.check_call([sys.executable, \\"-m\\", \\"pip\\", \\"install\\", pacote])\\n",
    "        print(f\\"‚úÖ {pacote} instalado com sucesso\\")\\n",
    "        return True\\n",
    "\\n",
    "# Verificar e instalar depend√™ncias\\n",
    "dependencias = ['flask', 'flask_cors']\\n",
    "\\n",
    "print(\\"üîç Verificando depend√™ncias...\\")\\n",
    "for dep in dependencias:\\n",
    "    instalar_se_necessario(dep)\\n",
    "\\n",
    "print(\\"\\\\n‚úÖ Todas as depend√™ncias est√£o prontas!\\")"
   ]
  },
  {
