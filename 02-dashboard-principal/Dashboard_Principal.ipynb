    "\\n",
    "@app.route('/api/executar/<modulo>', methods=['POST'])\\n",
    "def api_executar(modulo):\\n",
    "    \\"\\"\\"API para executar concilia√ß√£o.\\"\\"\\"\\n",
    "    print(f\\"üöÄ Executando m√≥dulo: {modulo}\\")\\n",
    "    \\n",
    "    # Simular execu√ß√£o\\n",
    "    time.sleep(1)  # Simular processamento\\n",
    "    \\n",
    "    return jsonify({\\n",
    "        \\"status\\": \\"iniciado\\",\\n",
    "        \\"modulo\\": modulo,\\n",
    "        \\"timestamp\\": datetime.now().isoformat(),\\n",
    "        \\"ambiente\\": ambiente\\n",
    "    })\\n",
    "\\n",
    "print(\\"‚úÖ Rotas da API configuradas!\\")\\n",
    "print(\\"   ‚Ä¢ / - Dashboard principal\\")\\n",
    "print(\\"   ‚Ä¢ /api/status - Status das concilia√ß√µes\\")\\n",
    "print(\\"   ‚Ä¢ /api/executar/<modulo> - Executar concilia√ß√£o\\")"
   ]
