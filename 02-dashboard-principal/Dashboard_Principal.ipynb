   "source": [
    "import os\\n",
    "import sys\\n",
    "from pathlib import Path\\n",
    "import json\\n",
    "\\n",
    "# Verificar se configura√ß√£o existe\\n",
    "config_file = Path(\\"../config_ambiente.json\\")\\n",
    "\\n",
    "if config_file.exists():\\n",
    "    print(\\"‚úÖ Configura√ß√£o encontrada!\\")\\n",
    "    with open(config_file, 'r', encoding='utf-8') as f:\\n",
    "        config_data = json.load(f)\\n",
    "    \\n",
    "    ambiente = config_data['environment']\\n",
    "    print(f\\"üéØ Ambiente detectado: {ambiente.upper()}\\")\\n",
    "    \\n",
    "    if ambiente == 'production':\\n",
    "        print(\\"üè≠ Modo PRODU√á√ÉO ativado\\")\\n",
    "        print(\\"   ‚Ä¢ Dados reais\\")\\n",
    "        print(\\"   ‚Ä¢ Banco compartilhado\\")\\n",
    "        print(\\"   ‚Ä¢ Interface verde\\")\\n",
    "    else:\\n",
    "        print(\\"üîß Modo DESENVOLVIMENTO ativado\\")\\n",
    "        print(\\"   ‚Ä¢ Dados simulados\\")\\n",
    "        print(\\"   ‚Ä¢ Banco local\\")\\n",
    "        print(\\"   ‚Ä¢ Interface azul\\")\\n",
    "else:\\n",
    "    print(\\"‚ö†Ô∏è Configura√ß√£o n√£o encontrada!\\")\\n",
    "    print(\\"üìã Execute primeiro o notebook da pasta 01-configuracao-ambiente\\")\\n",
    "    ambiente = 'development'  # Default"
   ]
  },
  {
