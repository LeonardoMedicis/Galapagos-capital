    "## üöÄ 5. Iniciar servidor e abrir navegador"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def abrir_navegador():\\n",
    "    \\"\\"\\"Abre navegador ap√≥s delay.\\"\\"\\"\\n",
    "    time.sleep(2)  # Aguardar servidor iniciar\\n",
    "    url = \\"http://localhost:5000\\"\\n",
    "    print(f\\"üåê Abrindo navegador: {url}\\")\\n",
    "    webbrowser.open(url)\\n",
    "\\n",
    "def iniciar_servidor():\\n",
    "    \\"\\"\\"Inicia servidor Flask.\\"\\"\\"\\n",
    "    print(\\"üöÄ Iniciando servidor dashboard...\\")\\n",
    "    print(\\"üìç URL: http://localhost:5000\\")\\n",
    "    print(\\"‚èπÔ∏è Para parar: Kernel > Interrupt\\")\\n",
    "    print(\\"=\\" * 50)\\n",
    "    \\n",
    "    # Abrir navegador em thread separada\\n",
    "    threading.Thread(target=abrir_navegador, daemon=True).start()\\n",
    "    \\n",
    "    # Iniciar servidor\\n",
    "    try:\\n",
    "        app.run(host='0.0.0.0', port=5000, debug=False, use_reloader=False)\\n",
    "    except KeyboardInterrupt:\\n",
    "        print(\\"\\\\nüëã Servidor encerrado pelo usu√°rio\\")\\n",
    "    except Exception as e:\\n",
    "        print(f\\"‚ùå Erro no servidor: {e}\\")\\n",
    "\\n",
    "# Iniciar servidor\\n",
    "iniciar_servidor()"
   ]
